{
    "name": "form-generator-app",
    "version": "0.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/Recursyve/ngx-form-generator.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build form-generator",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:lib": "ng build form-generator -c production",
        "publish-package": "npm run build:lib && npm publish ./dist/form-generator --access public",
        "publish-package:beta": "npm run build:lib && npm publish ./dist/form-generator --tag beta --access public",
        "publish-package:dry-run": "npm run build:lib && npm publish ./dist/form-generator --dry-run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.2.13",
        "@angular/common": "^18.2.13",
        "@angular/compiler": "^18.2.13",
        "@angular/core": "^18.2.13",
        "@angular/forms": "^18.2.13",
        "@angular/platform-browser": "^18.2.13",
        "@angular/platform-browser-dynamic": "^18.2.13",
        "@angular/router": "^18.2.13",
        "core-js": "^3.21.1",
        "rxjs": "^7.4.0",
        "tslib": "^2.4.0",
        "zone.js": "~0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.12",
        "@angular/cli": "^18.2.12",
        "@angular/compiler-cli": "^18.2.13",
        "@angular/language-service": "^18.2.13",
        "@types/node": "^12.11.1",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.20",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^18.2.1",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~5.4.5"
    }
}
