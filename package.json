{
    "name": "form-generator-app",
    "version": "0.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/Recursyve/ngx-form-generator.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build form-generator",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:lib": "ng build form-generator -c production",
        "publish-package": "npm run build:lib && npm publish dist/form-generator --access public",
        "publish-package:beta": "npm run build:lib && npm publish dist/form-generator --tag beta --access public",
        "publish-package:dry-run": "npm run build:lib && npm publish dist/form-generator --dry-run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.0.0",
        "@angular/common": "~12.0.0",
        "@angular/compiler": "~12.0.0",
        "@angular/core": "~12.0.0",
        "@angular/forms": "~12.0.0",
        "@angular/platform-browser": "~12.0.0",
        "@angular/platform-browser-dynamic": "~12.0.0",
        "@angular/router": "~12.0.0",
        "core-js": "^2.5.4",
        "rxjs": "~6.5.4",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.0.0",
        "@angular/cli": "~12.0.0",
        "@angular/compiler-cli": "~12.0.0",
        "@angular/language-service": "~12.0.0",
        "@types/node": "^12.11.1",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^12.0.0",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~4.2.4"
    }
}
